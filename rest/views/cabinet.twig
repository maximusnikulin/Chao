{% extends './base.twig' %}

{% block content %}
  <ul>
    <li>Login {{user.login}}</li>
    <li>Name {{user.name}}</li>    
  </ul>
  <div>
    <h1> List Rooms</h1>
    <ul>      
      {% for room in rooms %}
        <li>
          <a href="/rooms/{{room.id}}">{{room.title}}</a>
        </li>
      {% endfor %}
    </ul>
  </div>
{% endblock %}
